if(window.name.includes("bz-client")||parent!=window){
  registerTab()
}

function registerTab(){
  try{
    chrome.runtime.sendMessage({registerTab: 1,name:window.name},function(_msg){
      if(_msg){
        bzTwComm.setAppInfo(_msg)
      }
    });
    return 1
  }catch(ex){
    return "Register failed:"+ex.message
  }
}